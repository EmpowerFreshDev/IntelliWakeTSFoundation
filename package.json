{
  "name": "@empowerfreshdev/intelliwake",
  "version": "1.0.0",
  "description": "EmpowerFresh fork of the IntelliWake TypeScript Foundation Components 5.12.58",
  "type": "module",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.umd.cjs"
    }
  },
  "scripts": {
    "test": "vitest",
    "dev": "ts-node-dev --script-mode --respawn --transpile-only consoles/consoles.ts",
    "build": "tsc && vite build",
    "prepare": "npm run build",
    "version:minor": "pnpm version minor",
    "publish:local": "npm run build && pnpm version patch && git push --tags && git push"
  },
  "files": [
    "dist"
  ],
  "author": "EmpowerFresh Team",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EmpowerFreshDev/IntelliWakeTSFoundation.git"
  },
  "devDependencies": {
    "@types/node": "^14.18.36",
    "prettier": "^2.8.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.9.5",
    "vite": "^4.5.2",
    "vite-plugin-dts": "^1.7.2",
    "vitest": "^0.33.0"
  }
}
