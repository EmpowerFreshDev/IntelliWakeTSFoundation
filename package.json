{
	"name": "@solidbasisventures/intelliwaketsfoundation",
	"version": "5.4.6",
	"description": "NPM package for IntelliWake TypeScript Foundation Components",
	"type": "module",
	"main": "dist/main.js",
	"types": "dist/main.d.ts",
	"exports": {
		".": {
			"import": "./dist/main.js",
			"require": "./dist/main.umd.cjs"
		}
	},
	"scripts": {
		"Vitest-Watch": "vitest",
		"TSNodeDev": "ts-node-dev --script-mode --respawn --transpile-only consoles/consoles.ts",
		"Publish": "tsc && vite build && pnpm version patch && git push && npm publish",
		"Install-IntelliWake": "pnpm install",
		"Version-Minor-Advance": "pnpm version minor"
	},
	"files": [
		"dist"
	],
	"author": "SolidBasisVentures",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/SolidBasisVentures/IntelliWakeTSFoundation.git"
	},
	"devDependencies": {
		"@types/node": "^14.18.36",
		"prettier": "^2.8.3",
		"ts-node": "^9.1.1",
		"ts-node-dev": "^1.1.8",
		"typescript": "^4.9.5",
		"vite": "^4.1.1",
		"vite-plugin-dts": "^1.7.2",
		"vitest": "^0.28.4"
	}
}
